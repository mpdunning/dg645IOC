  field(TWVL, "2")
  field(THVL, "3")
  field(FRVL, "4")
  field(FVVL, "5")
  field(SXVL, "6")
  field(ZRST, "Internal")
  field(ONST, "External Rising Edge")
  field(TWST, "External Falling Edge")
  field(THST, "Single Ext. Ris. Edge")
  field(FRST, "Single Ext. Fal. Edge")
  field(FVST, "Single")
