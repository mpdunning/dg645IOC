# Init and read sequences ---------------------------
record(seq, "$(P):SEQ0"){
  field(DESC, "Initialization sequence")
  field(LNK1, "$(P):IDN.PROC")
  field(DLY1, "$(DELAY0)")
  field(LNK2, "$(P):ERROR.PROC")
  field(DLY2, "$(DELAY1)")
  field(LNK3, "$(P):SEQ1.PROC")
  field(DLY3, "$(DELAY1)")
  field(PINI, "1")
  #field(SCAN, "$(SCAN0)")
}
